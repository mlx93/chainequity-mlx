{
  "name": "chainequity-indexer",
  "version": "1.0.0",
  "description": "ChainEquity blockchain event indexer for GatedToken contract",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "mkdir -p dist/db && cp src/db/schema.sql dist/db/ && mkdir -p dist/abis && cp src/abis/GatedToken.json dist/abis/",
    "start": "node dist/index.js",
    "init-db": "ts-node src/db/initSchema.ts"
  },
  "keywords": ["blockchain", "indexer", "ethereum", "base-sepolia"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1",
    "viem": "^2.21.57"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
